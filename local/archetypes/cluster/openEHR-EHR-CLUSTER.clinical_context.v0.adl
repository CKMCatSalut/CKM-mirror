definition
	CLUSTER[at0000] occurrences matches {0..1} matches {    -- Clinical context
		items cardinality matches {1..*; unordered} matches {
			CLUSTER[at0029] occurrences matches {0..*} matches {    -- Diagnostic
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0030] occurrences matches {0..1} matches {    -- Diagnostic name
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0031] occurrences matches {0..*} matches {    -- Diagnostic date
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
				}
			}
			CLUSTER[at0032] occurrences matches {0..*} matches {    -- Health indicators
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0033] occurrences matches {0..1} matches {    -- Health indicator name
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0034] occurrences matches {0..*} matches {    -- Health indicator date
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
				}
			}
			CLUSTER[at0035] occurrences matches {0..1} matches {    -- Clinical Episode
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0036] occurrences matches {0..1} matches {    -- Episode name
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0037] occurrences matches {0..*} matches {    -- Episode date
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
				}
			}
			CLUSTER[at0038] occurrences matches {0..*} matches {    -- Allergy or Adverse reaction
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0039] occurrences matches {0..1} matches {    -- Allergy or Adverse reaction
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0040] occurrences matches {0..*} matches {    -- Allergy or Adverse reaction date
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
				}
			}
			allow_archetype CLUSTER[at0019] occurrences matches {0..1} matches {    -- Additional details
				include
					archetype_id/value matches {/.*/}
			}
		}
	}


