													>
													["2"] = <
														units = <"[degF]">
													>
												>
											>
										}
									}
									ELEMENT[at0005] occurrences matches {0..1} matches {    -- Lloc
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
