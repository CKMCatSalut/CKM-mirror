">
				>
				["at0142"] = <
					text = <"Kategori">
					description = <"Kategorin av läkemedlet eller läkemedelskomponenten med avseende på tillverkning eller beredning, och antalet ingredienser.">
					comment = <"Till exempel: \"Paracetamol/codeine\" är en produkt med flera ingredienser, medan \"Morfin 60 mg + Haloperidol 2 mg + Midazolam 5 mg\" är en situationsanpassad blandning vars sammansättning är fullständigt specificerad i beställningen.">
				>
				["at0143"] = <
					text = <"Situationsanpassad blandning">
					description = <"Läkemedlet eller läkemedelskomponenten består av en blandning av ingredienser som anges i beställningen. Dessa bereds vanligtvis av apotek eller avdelningspersonal för att anpassas till enskilda patienter">
				>
				["at0144"] = <
					text = <"Flerkomponent produkt">
					description = <"Läkemedlet eller läkemedelskomponenten består av ett antal aktiva ingredienser som i förväg kombineras till en enda beredningsform som en tablett eller pulver av tillverkaren, till exempel Paracetamol / kodein.">
				>
				["at0145"] = <
					text = <"Singelsubstans produkt">
					description = <"Läkemedlet eller läkemedelskomponenten är en tillverkad produkt som innehåller en enda aktiv beståndsdel.">
				>
				["at0146"] = <
					text = <"Beståndsdel">
					description = <"Läkemedlet eller läkemedelskomponenten är en enskild beståndsdel i läkemedlet. Denna term används när arketypen är inkapslad i en överliggande instans av sig själv, för att beskriva de enskilda beståndsdelarna i ett läkemedel.">
				>
				["at0148"] = <
					text = <"Alternativ mängd">
					description = <"En alternativt sätt att beskriva mängden av läkemedlet eller läkemedelskomponenten.">
					comment = <"Till exempel för ett läkemedel med styrkan \"5 mg/ml\" är mängden \"1 ml\", men värdet som skall registreras i detta fält är '5 mg'.">
				>
				["at0150"] = <
					text = <"Batch ID">
					description = <"Den identifierare som tillverkaren tilldelat batchen under produktionen.">
				>
				["at0151"] = <
					text = <"Tillverkare">
					description = <"Tillverkaren av läkemedlet eller läkemedelskomponenten.">
					comment = <"Till exempel: \"Abbott\".">
				>
				["at0152"] = <
					text = <"Styrka (beskrivning)">
					description = <"Styrkan hos läkemedlet eller läkemedelskomponenten, uttryckt som ett förhållande.">
					comment = <"I vissa fall, som för flytande eller halvfasta läkemedel, anges i nämnaren en beskrivning av ett styrkeförhållande, en fysisk mängd, till exempel 2 mg/5 ml. Ibland återspeglar nämnaren också den faktiska volymen av komponenten till 5 ml i det tidigare exemplet. I detta fall skulle \"Styrka (koncentration)\" vara 0,4 mg/ml. I andra fall, där styrkan involverar en nämnare som inte är en fysisk storhet, till exempel 4 mg/tablett, uttrycks nämnaren som ett enhetligt värde \"1\" med enheten \"tablett\". Detta tillvägagångssätt valdes för att överensstämma med den metod som antagits av ISO IDMP-standarden för läkemedelskataloger. Om fältet \"Styrka (koncentration)\" används blir detta kluster överflödigt.">
				>
				["at0153"] = <
					text = <"Styrka i täljare">
					description = <"Täljaren vid en beskrivning av styrkeförhållandet">
					comment = <"Till exempel när styrkan anges som \"300 µg/0,3 ml\" är uppgiften om styrkan i täljaren \"300 ug\". För en angiven styrka på \"100 mg/tablett\" är uppgiften om styrkan \"100 mg\".">
				>
				["at0157"] = <
					text = <"Styrka i nämnare">
					description = <"Nämnaren vid en beskrivning av styrkeförhållandet.">
					comment = <"Till exempel när styrkan anges som \"300 µg/0,3 ml\" är uppgiften om styrkan i nämnaren \"0,3 ml\". För en angiven styrka på \"100 mg/tablett\" är uppgiften om styrkan \"1 tablett\".">
				>
			>
		>
		["ca"] = <
			items = <
				["at0000"] = <
					text = <"Detalls de la medicació">
					description = <"Dades sobre un medicament o un component d'un medicament, inclosa la concentració, la forma i els detalls de qualsevol component específic.">
