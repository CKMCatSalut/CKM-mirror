					description = <"Información adicional sobre el evento de reacción adversa no recogida en otros campos.">
				>
				["at0025"] = <
					text = <"Información de soporte de la historia clínica">
					description = <"Enlace a la información complementaria, con informes de pruebas de alergias, sobre la presentación y las observaciones que se encuentran en otra parte de la historia clínica.">
					comment = <"Por ejemplo, síntomas presentes, observaciones de exámenes, diagnósticos,etc.">
				>
				["at0026"] = <
					text = <"No confirmado">
					description = <"No se ha confirmado con objetividad que la \"Manifestación\" de este evento de reacción la causara la exposición a la \"Sustancia\" identificada.">
				>
				["at0027"] = <
					text = <"Refutado">
					description = <"Se ha cuestionado o desmentido que la \"Manifestación\" de este evento de reacción la causara la exposición a la \"Sustancia\" identificada, y con un grado suficiente de certeza clínica para justificar la invalidez de esta afirmación. Esto puede incluir indicios clínicos mediante pruebas, reexposición u observación.">
