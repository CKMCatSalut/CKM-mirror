				["at0021"] = <
					text = <"Detalles de la gestión clínica">
					description = <"Detalles adicionales estructurados de la gestión clínica relacionados con este evento de reacción.">
				>
				["at0022"] = <
					text = <"Detalles del informe">
					description = <"Detalles adicionales estructurados requeridos para el informe destinados a organismos reguladores.">
				>
				["at0023"] = <
					text = <"Fuente de información">
					description = <"Detalles sobre la procedencia de la información.">
					comment = <"Este SLOT pretende proporcionar datos sobre la fuente de la información de este \"Evento de reacción\" concrecto.">
				>
				["at0024"] = <
