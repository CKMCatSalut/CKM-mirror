											2|[local::at0127],
											3|[local::at0128],
											4|[local::at0129],
											5|[local::at0130]
										}
									}
									ELEMENT[at0131] occurrences matches {0..1} matches {    -- 5 Duration of daytime pain
										value matches {
											1|[local::at0132],
											2|[local::at0133],
											3|[local::at0134],
											4|[local::at0135],
											5|[local::at0136]
										}
									}
									ELEMENT[at0137] occurrences matches {0..1} matches {    -- 6 Numbness
										value matches {
											1|[local::at0138],
											2|[local::at0139],
											3|[local::at0140],
											4|[local::at0141],
											5|[local::at0142]
										}
									}
									ELEMENT[at0143] occurrences matches {0..1} matches {    -- 7 Weakness
										value matches {
											1|[local::at0144],
											2|[local::at0145],
											3|[local::at0146],
											4|[local::at0147],
											5|[local::at0148]
										}
									}
									ELEMENT[at0149] occurrences matches {0..1} matches {    -- 8 Tingling
										value matches {
											1|[local::at0150],
											2|[local::at0151],
											3|[local::at0152],
											4|[local::at0153],
											5|[local::at0154]
										}
									}
									ELEMENT[at0155] occurrences matches {0..1} matches {    -- 9 Numbness or Tingling at night
										value matches {
											1|[local::at0156],
											2|[local::at0157],
											3|[local::at0158],
											4|[local::at0159],
											5|[local::at0160]
										}
									}
									ELEMENT[at0161] occurrences matches {0..1} matches {    -- 10 Frequency waking due to numbness
										value matches {
											1|[local::at0162],
											2|[local::at0163],
											3|[local::at0164],
											4|[local::at0165],
											5|[local::at0166]
										}
									}
									ELEMENT[at0167] occurrences matches {0..1} matches {    -- 11 Difficulty grasping
										value matches {
