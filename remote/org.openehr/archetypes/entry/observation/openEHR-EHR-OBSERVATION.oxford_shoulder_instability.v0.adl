definition
	OBSERVATION[at0000] matches {    -- Oxford Shoulder Instability Questionnaire Score (OSI)
		data matches {
			HISTORY[at0001] matches {    -- Event Series
				events cardinality matches {1..*; unordered} matches {
					POINT_EVENT[at0002] occurrences matches {0..1} matches {    -- Point in time
						data matches {
							ITEM_TREE[at0003] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0008] occurrences matches {0..1} matches {    -- 1 Slipped out of joint
										value matches {
											0|[local::at0109],
											1|[local::at0009],
											2|[local::at0010],
											3|[local::at0011],
											4|[local::at0012]
										}
									}
									ELEMENT[at0014] occurrences matches {0..1} matches {    -- 2 Trouble dressing
										value matches {
											0|[local::at0015],
											1|[local::at0016],
											2|[local::at0017],
											3|[local::at0018],
											4|[local::at0019]
										}
									}
									ELEMENT[at0020] occurrences matches {0..1} matches {    -- 3 Worst Pain
										value matches {
											0|[local::at0021],
											1|[local::at0022],
											2|[local::at0023],
											3|[local::at0024],
											4|[local::at0025]
										}
									}
									ELEMENT[at0026] occurrences matches {0..1} matches {    -- 4 Work
										value matches {
											0|[local::at0027],
											1|[local::at0028],
											2|[local::at0029],
											3|[local::at0030],
											4|[local::at0031]
										}
									}
									ELEMENT[at0032] occurrences matches {0..1} matches {    -- 5 Activities
										value matches {
											0|[local::at0033],
											1|[local::at0034],
											2|[local::at0035],
											3|[local::at0036],
											4|[local::at0037]
										}
									}
									ELEMENT[at0115] occurrences matches {0..1} matches {    -- 6 Important things
										value matches {
											0|[local::at0116],
											1|[local::at0117],
											2|[local::at0118],
											3|[local::at0119],
											4|[local::at0120]
										}
									}
									ELEMENT[at0121] occurrences matches {0..1} matches {    -- 7 Social Life
										value matches {
											0|[local::at0122],
											1|[local::at0123],
											2|[local::at0124],
											3|[local::at0125],
											4|[local::at0126]
										}
									}
									ELEMENT[at0127] occurrences matches {0..1} matches {    -- 8 Sport or hobbies
										value matches {
											0|[local::at0128],
											1|[local::at0129],
											2|[local::at0130],
											3|[local::at0131],
											4|[local::at0132]
										}
									}
									ELEMENT[at0133] occurrences matches {0..1} matches {    -- 9 Thought about shoulder
										value matches {
											0|[local::at0134],
											1|[local::at0135],
											2|[local::at0136],
											3|[local::at0137],
											4|[local::at0138]
										}
									}
									ELEMENT[at0139] occurrences matches {0..1} matches {    -- 10 Heavy objects
										value matches {
											0|[local::at0140],
											1|[local::at0141],
											2|[local::at0142],
											3|[local::at0143],
											4|[local::at0144]
										}
									}
									ELEMENT[at0145] occurrences matches {0..1} matches {    -- 11 Pain
										value matches {
											0|[local::at0146],
											1|[local::at0147],
											2|[local::at0148],
											3|[local::at0149],
											4|[local::at0150]
										}
									}
									ELEMENT[at0151] occurrences matches {0..1} matches {    -- 12 Sleep position
										value matches {
											0|[local::at0152],
											1|[local::at0153],
											2|[local::at0154],
											3|[local::at0155],
											4|[local::at0156]
										}
									}
									ELEMENT[at0104] occurrences matches {0..1} matches {    -- Total score
										value matches {
											DV_COUNT matches {
												magnitude matches {|0..48|}
											}
										}
									}
									ELEMENT[at0105] occurrences matches {0..1} matches {    -- Average score
										value matches {
											DV_COUNT matches {
												magnitude matches {|0..100|}
											}
										}
									}
									ELEMENT[at0157] occurrences matches {0..1} matches {    -- Grading
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0158,    -- Poor
													at0159,    -- Fair
													at0160,    -- Good
													at0161]    -- Excellent
												}
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0005] matches {    -- Tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0006] occurrences matches {0..1} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

