definition
	OBSERVATION[at0000] matches {    -- Oxford Hip Questionnaire Score (OHS)
		data matches {
			HISTORY[at0001] matches {    -- Event Series
				events cardinality matches {1..*; unordered} matches {
					POINT_EVENT[at0002] occurrences matches {0..1} matches {    -- Point in time
						data matches {
							ITEM_TREE[at0003] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0008] occurrences matches {0..1} matches {    -- 1 Usual pain
										value matches {
											0|[local::at0109],
											1|[local::at0009],
											2|[local::at0010],
											3|[local::at0011],
											4|[local::at0012]
										}
									}
									ELEMENT[at0014] occurrences matches {0..1} matches {    -- 2 Washing and drying
										value matches {
											0|[local::at0019],
											1|[local::at0015],
											2|[local::at0018],
											3|[local::at0017],
											4|[local::at0016]
										}
									}
									ELEMENT[at0020] occurrences matches {0..1} matches {    -- 3 Vehicle use
										value matches {
											0|[local::at0021],
											1|[local::at0022],
											2|[local::at0023],
											3|[local::at0024],
											4|[local::at0025]
										}
									}
									ELEMENT[at0026] occurrences matches {0..1} matches {    -- 4 Socks
										value matches {
											0|[local::at0027],
											1|[local::at0028],
											2|[local::at0029],
											3|[local::at0030],
											4|[local::at0031]
										}
									}
									ELEMENT[at0032] occurrences matches {0..1} matches {    -- 5 Household shopping
										value matches {
											0|[local::at0033],
											1|[local::at0034],
											2|[local::at0035],
											3|[local::at0036],
											4|[local::at0037]
										}
									}
									ELEMENT[at0038] occurrences matches {0..1} matches {    -- 6 Walk duration
										value matches {
											0|[local::at0039],
											1|[local::at0040],
											2|[local::at0041],
											3|[local::at0042],
											4|[local::at0043]
										}
									}
									ELEMENT[at0044] occurrences matches {0..1} matches {    -- 7 Stairs
										value matches {
											0|[local::at0110],
											1|[local::at0111],
											2|[local::at0112],
											3|[local::at0113],
											4|[local::at0114]
										}
									}
									ELEMENT[at0050] occurrences matches {0..1} matches {    -- 8 Standing after meal
										value matches {
											0|[local::at0051],
											1|[local::at0052],
											2|[local::at0053],
											3|[local::at0054],
											4|[local::at0055]
										}
									}
									ELEMENT[at0056] occurrences matches {0..1} matches {    -- 9 Limping
										value matches {
											0|[local::at0057],
											1|[local::at0058],
											2|[local::at0059],
											3|[local::at0060],
											4|[local::at0061]
										}
									}
									ELEMENT[at0062] occurrences matches {0..1} matches {    -- 10 Sudden pain
										value matches {
											0|[local::at0063],
											1|[local::at0064],
											2|[local::at0065],
											3|[local::at0066],
											4|[local::at0067]
										}
									}
									ELEMENT[at0068] occurrences matches {0..1} matches {    -- 11 Interfered with work
										value matches {
											0|[local::at0069],
											1|[local::at0070],
											2|[local::at0071],
											3|[local::at0072],
											4|[local::at0073]
										}
									}
									ELEMENT[at0074] occurrences matches {0..1} matches {    -- 12 Night pain
										value matches {
											0|[local::at0075],
											1|[local::at0076],
											2|[local::at0077],
											3|[local::at0078],
											4|[local::at0079]
										}
									}
									ELEMENT[at0104] occurrences matches {0..1} matches {    -- Total score
										value matches {
											DV_COUNT matches {
												magnitude matches {|0..48|}
											}
										}
									}
									ELEMENT[at0105] occurrences matches {0..1} matches {    -- Average score
										value matches {
											DV_COUNT matches {
												magnitude matches {|0..100|}
											}
										}
									}
									ELEMENT[at0115] occurrences matches {0..1} matches {    -- Grading
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0116,    -- Severe
													at0117,    -- Moderate to severe
													at0118,    -- Moderate
													at0119]    -- Satisfactory
												}
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0005] matches {    -- Tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0006] occurrences matches {0..1} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

