											2|[local::at0035],
											3|[local::at0036],
											4|[local::at0037]
										}
									}
									ELEMENT[at0038] occurrences matches {0..1} matches {    -- 6 Carry a tray
										value matches {
											0|[local::at0039],
											1|[local::at0040],
											2|[local::at0041],
											3|[local::at0042],
											4|[local::at0043]
										}
									}
									ELEMENT[at0044] occurrences matches {0..1} matches {    -- 7 Brush hair
										value matches {
											0|[local::at0110],
											1|[local::at0111],
											2|[local::at0112],
											3|[local::at0113],
											4|[local::at0114]
