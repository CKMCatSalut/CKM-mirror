											4|[local::at0198]
										}
									}
									ELEMENT[at0199] occurrences matches {0..1} matches {    -- 9 Interfered with work
										value matches {
											0|[local::at0200],
											1|[local::at0201],
											2|[local::at0202],
											3|[local::at0203],
											4|[local::at0204]
										}
