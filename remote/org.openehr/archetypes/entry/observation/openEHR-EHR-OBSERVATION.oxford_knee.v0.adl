											2|[local::at0029],
											3|[local::at0030],
											4|[local::at0031]
										}
									}
									ELEMENT[at0032] occurrences matches {0..1} matches {    -- 5 Stand from table
										value matches {
											0|[local::at0033],
											1|[local::at0034],
											2|[local::at0035],
											3|[local::at0036],
											4|[local::at0037]
										}
									}
									ELEMENT[at0056] occurrences matches {0..1} matches {    -- 6 Limping
										value matches {
											0|[local::at0057],
											1|[local::at0058],
											2|[local::at0059],
											3|[local::at0060],
											4|[local::at0061]
										}
									}
									ELEMENT[at0120] occurrences matches {0..1} matches {    -- 7 Kneeling
										value matches {
											0|[local::at0121],
											1|[local::at0122],
											2|[local::at0123],
											3|[local::at0124],
											4|[local::at0125]
										}
									}
									ELEMENT[at0074] occurrences matches {0..1} matches {    -- 8 Night pain
										value matches {
											0|[local::at0075],
											1|[local::at0076],
											2|[local::at0077],
											3|[local::at0078],
											4|[local::at0079]
										}
									}
									ELEMENT[at0068] occurrences matches {0..1} matches {    -- 9 Interfered with work
										value matches {
											0|[local::at0069],
											1|[local::at0070],
											2|[local::at0071],
											3|[local::at0072],
											4|[local::at0073]
										}
									}
									ELEMENT[at0062] occurrences matches {0..1} matches {    -- 10 Sudden pain
										value matches {
											0|[local::at0063],
											1|[local::at0064],
											2|[local::at0065],
											3|[local::at0066],
											4|[local::at0067]
										}
									}
									ELEMENT[at0044] occurrences matches {0..1} matches {    -- 11 Shopping
										value matches {
											0|[local::at0110],
											1|[local::at0111],
											2|[local::at0112],
											3|[local::at0113],
											4|[local::at0114]
										}
									}
									ELEMENT[at0126] occurrences matches {0..1} matches {    -- 12 Stairs
										value matches {
											0|[local::at0127],
											1|[local::at0128],
											2|[local::at0129],
											3|[local::at0130],
											4|[local::at0131]
										}
									}
									ELEMENT[at0104] occurrences matches {0..1} matches {    -- Total score
										value matches {
											DV_COUNT matches {
												magnitude matches {|0..48|}
											}
										}
									}
									ELEMENT[at0105] occurrences matches {0..1} matches {    -- Average score
										value matches {
											DV_COUNT matches {
												magnitude matches {|0..100|}
											}
										}
									}
									ELEMENT[at0115] occurrences matches {0..1} matches {    -- Grading
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0116,    -- Severe
													at0117,    -- Moderate to severe
													at0118,    -- Moderate
													at0119]    -- Satisfactory
												}
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0005] matches {    -- Tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0006] occurrences matches {0..1} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

