											0|[local::at0019],
											1|[local::at0015],
											2|[local::at0018],
											3|[local::at0017],
											4|[local::at0016]
										}
									}
									ELEMENT[at0020] occurrences matches {0..1} matches {    -- 3 Vehicle use
										value matches {
											0|[local::at0021],
											1|[local::at0022],
											2|[local::at0023],
											3|[local::at0024],
											4|[local::at0025]
										}
									}
									ELEMENT[at0026] occurrences matches {0..1} matches {    -- 4 Walking duration
										value matches {
											0|[local::at0027],
