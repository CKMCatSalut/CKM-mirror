				["at0045"] = <
					text = <"Administration outcome">
					description = <"The observed response to the administration of vaccine.">
					comment = <"Coding with an external terminology is preferred, where possible. For example: a reaction such as rash or shortness of breath.">
				>
				["at0046"] = <
