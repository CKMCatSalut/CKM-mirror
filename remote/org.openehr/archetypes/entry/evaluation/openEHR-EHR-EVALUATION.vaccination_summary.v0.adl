definition
	EVALUATION[at0000] matches {    -- Vaccination summary
		data matches {
			ITEM_TREE[at0001] matches {    -- Tree
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0052] occurrences matches {0..1} matches {    -- Vaccine name
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0002] occurrences matches {0..*} matches {    -- Targeted disease or agent
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0035] occurrences matches {0..1} matches {    -- Description
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0003] occurrences matches {0..1} matches {    -- Primary course status
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0004,    -- Course not commenced
									at0005,    -- Course incomplete
									at0006,    -- Course complete
									at0007]    -- Unknown
								}
							}
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0008] occurrences matches {0..1} matches {    -- Date course completed
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					ELEMENT[at0047] occurrences matches {0..1} matches {    -- Total number of administrations
						value matches {
							DV_COUNT matches {
								magnitude matches {|>=0|}
							}
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::at0048]    -- Unknown
								}
							}
						}
					}
					ELEMENT[at0009] occurrences matches {0..1} matches {    -- Date of last vaccination
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					ELEMENT[at0033] occurrences matches {0..1} matches {    -- Next vaccination due
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					allow_archetype CLUSTER[at0051] occurrences matches {0..1} matches {    -- Additional details
					}
					ELEMENT[at0049] occurrences matches {0..1} matches {    -- Comment
						value matches {
							DV_TEXT matches {*}
						}
					}
					CLUSTER[at0022] occurrences matches {0..*} matches {    -- Per vaccine administration
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0023] occurrences matches {0..*} matches {    -- Specific vaccine name
								value matches {
									DV_TEXT matches {*}
								}
							}
							allow_archetype CLUSTER[at0034] occurrences matches {0..*} matches {    -- Vaccine details
								include
									archetype_id/value matches {/openEHR-EHR-CLUSTER\.medication(-[a-zA-Z0-9_]+)*\.v1/}
							}
							ELEMENT[at0038] occurrences matches {0..1} matches {    -- Administration category
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0039,    -- Primary course
											at0040]    -- Booster
										}
									}
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0024] occurrences matches {0..1} matches {    -- Date of administration
								value matches {
									DV_DATE_TIME matches {*}
								}
							}
							ELEMENT[at0031] occurrences matches {0..1} matches {    -- Sequence in series
								value matches {
									DV_COUNT matches {
										magnitude matches {|>=1|}
									}
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0042] occurrences matches {0..1} matches {    -- Description
								value matches {
									DV_TEXT matches {*}
								}
							}
							allow_archetype CLUSTER[at0027] occurrences matches {0..*} matches {    -- Administration location
								include
									archetype_id/value matches {/openEHR-EHR-CLUSTER\.organisation(-[a-zA-Z0-9_]+)*\.v1/}
							}
							ELEMENT[at0045] occurrences matches {0..1} matches {    -- Administration outcome
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0046] occurrences matches {0..1} matches {    -- Information source
								value matches {
									DV_TEXT matches {*}
								}
							}
							allow_archetype CLUSTER[at0044] occurrences matches {0..*} matches {    -- Additional details
								include
									archetype_id/value matches {/openEHR-EHR-CLUSTER\.dosage(-[a-zA-Z0-9_]+)*\.v2/}
							}
							ELEMENT[at0016] occurrences matches {0..1} matches {    -- Administration comment
								value matches {
									DV_TEXT matches {*}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0013] matches {    -- Tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0015] occurrences matches {0..1} matches {    -- Last updated
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					ELEMENT[at0053] occurrences matches {0..1} matches {    -- Information source
						value matches {
							DV_TEXT matches {*}
						}
					}
					allow_archetype CLUSTER[at0050] occurrences matches {0..*} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

ontology
	term_definitions = <
		["en"] = <
			items = <
				["at0000"] = <
					text = <"Vaccination summary">
					description = <"Summary of vaccination administration for a single infectious disease or agent, or a group of infectious diseases or agents that are vaccinated simultaneously using a combination vaccine.">
				>
				["at0001"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Targeted disease or agent">
					description = <"Name of the infectious disease or agent targeted by the vaccine.">
					comment = <"If the 'Vaccine name' is a combination vaccine this data element may contain more than one target disease. Coding with an external terminology is preferred, where possible.">
				>
				["at0003"] = <
					text = <"Primary course status">
					description = <"Status of the primary (or catch up) course of vaccinations.">
					comment = <"This status should be revised if the course has not been completed and the 'Next vaccination due' date or 'Next review due' has passed. ">
				>
				["at0004"] = <
					text = <"Course not commenced">
					description = <"A course of vaccinations is planned but not commenced.">
				>
				["at0005"] = <
					text = <"Course incomplete">
					description = <"A course of vaccinations has been commenced but not completed.">
				>
				["at0006"] = <
					text = <"Course complete">
					description = <"A course of vaccinations has been completed.">
				>
				["at0007"] = <
					text = <"Unknown">
					description = <"It is not possible to determine if the primary course has been completed.">
				>
				["at0008"] = <
					text = <"Date course completed">
					description = <"The date on which the (primary or catch-up) course of vaccines was completed.">
				>
				["at0009"] = <
					text = <"Date of last vaccination">
					description = <"The date when the most recent vaccine was administered.">
					comment = <"Can be a partial date, for example, only a year.">
				>
				["at0013"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0015"] = <
					text = <"Last updated">
					description = <"The date on which the immunisation summary was last updated.">
				>
				["at0016"] = <
					text = <"Administration comment">
					description = <"Additional narrative about the vaccine administration for the identified vaccinet, not captured in other fields.">
				>
				["at0022"] = <
					text = <"Per vaccine administration">
					description = <"Details about a specific vaccine administration.">
				>
				["at0023"] = <
					text = <"Specific vaccine name">
					description = <"Name of the specific vaccine administered.">
					comment = <"For example: a trade name that may be more specific than  the index 'Vaccine name'. ">
				>
				["at0024"] = <
					text = <"Date of administration">
					description = <"Date of administration of the specified vaccine.">
					comment = <"Can be a partial date, for example, only a year.">
				>
				["at0027"] = <
					text = <"Administration location">
					description = <"">
				>
				["at0031"] = <
					text = <"Sequence in series">
					description = <"The sequence of the vaccine administered within a primary course or series.">
					comment = <"For example: 1/2 for first vaccination in a series of 2.">
				>
				["at0033"] = <
					text = <"Next vaccination due">
					description = <"The date when the next vaccine is due to be administered.">
